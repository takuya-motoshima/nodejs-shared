{
  "name": "nodejs-shared",
  "version": "1.0.26",
  "description": "This is a shared module of node.js.",
  "main": "dist/build.common.js",
  "module": "dist/build.esm.js",
  "types": "types/index.d.ts",
  "files": [
    "dist/*.js",
    "types",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "watch": "rollup -c rollup.config.js --watc",
    "prewatch": "rimraf dist types",
    "build": "rollup -c rollup.config.js",
    "prebuild": "rimraf dist types",
    "test": "jest",
    "test:clear-cache": "jest --clearCache",
    "//watch": "rollup -c --watch --bundleConfigAsCjs",
    "//build": "rollup -c --bundleConfigAsCjs"
  },
  "_moduleAliases": {
    "~": "."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takuya-motoshima/nodejs-shared.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "javascript",
    "js",
    "shared",
    "library",
    "lib",
    "module",
    "typescript",
    "ts",
    "esm",
    "es6"
  ],
  "author": "Takuya Motoshima <developer.takuyamotoshima@gmail.com> (https://twitter.com/TakuyaMotoshima)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/takuya-motoshima/nodejs-shared/issues",
    "email": "developer.takuyamotoshima@gmail.com"
  },
  "homepage": "https://github.com/takuya-motoshima/nodejs-shared#readme",
  "dependencies": {
    "bowser": "^2.10.0",
    "csv-parse": "^5.5.0",
    "deep-fusion": "^1.0.3",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "gm": "^1.25.0",
    "image-size": "^0.8.3",
    "mime-types": "^2.1.27",
    "moment": "^2.26.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.33.3",
    "uniqid": "^5.2.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.7",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.1.2",
    "@types/gm": "^1.25.0",
    "@types/jest": "^29.2.4",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.0.22",
    "@types/sharp": "^0.32.0",
    "@types/uniqid": "^5.2.0",
    "@types/validator": "^13.11.9",
    "builtin-modules": "^3.1.0",
    "jest": "^29.3.1",
    "module-alias": "^2.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-loader": "^9.4.2",
    "tsc-alias": "^1.8.2",
    "typescript": "^5.2.2"
  }
}
